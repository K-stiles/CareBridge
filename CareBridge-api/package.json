{
    "name": "carebridge-api",
    "version": "1.0.0",
    "description": "deployed url: https://pcareapi.cyclic.cloud/api/v1/api-docs",
    "main": "index.js",
    "scripts": {
        "start-db": "docker-compose up -d",
        "seed-db": "npx prisma migrate dev --name init",
        "generate-swagger": "swagger-jsdoc -d swagger.json -o src/docs/swagger-jsdoc-ts",
        "build": "rimraf dist && prettier --write src/ && npx tsc",
        "prestart": "yarn build",
        "start": "node dist/index.js",
        "postinstall": "npx tsc",
        "predev": "yarn build",
        "dev": "concurrently \"npx tsc -w\" \"nodemon dist/index.js\" ",
        "lint": "eslint --ext .js,.ts .",
        "format": "npx prisma format",
        "prettier": "prettier --write \"**/*.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brytemenz/carebridgeapi.git"
    },
    "keywords": [],
    "author": "@kwakustiles",
    "license": "MIT",
    "private": true,
    "bugs": {
        "url": "https://github.com/brytemenz/carebridgeapi/issues"
    },
    "homepage": "https://github.com/brytemenz/carebridgeapi#readme",
    "dependencies": {
        "@prisma/client": "^5.5.2",
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "chalk": "4.0.0",
        "concurrently": "^8.2.0",
        "config": "^3.3.9",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dayjs": "^1.11.9",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^4.0.2",
        "pino": "^8.15.0",
        "pino-pretty": "^10.2.0",
        "rimraf": "^5.0.1",
        "stream": "^0.0.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.2",
        "@types/config": "^3.3.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/ejs": "^3.1.2",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/lodash": "^4.14.197",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.2",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "eslint": "^8.47.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.2",
        "prisma": "^5.5.2",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    },
    "prisma": {
        "seed": "ts-node src/db/seed.ts"
    }
}
